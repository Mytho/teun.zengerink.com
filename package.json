{
  "name": "teun.zengerink.com",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "next build && next export",
    "dev": "next dev",
    "fix": "yarn lint --fix",
    "lint": "yarn lint:ts && yarn lint:types",
    "lint:ts": "eslint src/**/*.{ts,tsx}",
    "lint:types": "tsc --noEmit",
    "postinstall": "husky install",
    "start": "next start",
    "test": "jest --watch",
    "test:smoke": "export $(grep -v '^#' .env | xargs) && curl -v --silent $NEXT_PUBLIC_PROTO://$NEXT_PUBLIC_HOST/ 2>&1 | grep -q $(git rev-parse HEAD)",
    "test:ci": "yarn lint && jest --ci --collectCoverage"
  },
  "dependencies": {
    "image-size": "^0.9.7",
    "js-yaml": "^4.0.0",
    "next": "^10.0.9",
    "next-build-id": "^3.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-markdown": "^5.0.3",
    "sass": "^1.32.8",
    "sharp": "^0.27.2",
    "slugify": "^1.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.13",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@testing-library/react": "^11.2.5",
    "@types/image-size": "^0.8.0",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.36",
    "@types/react": "^17.0.3",
    "@typescript-eslint/parser": "^4.19.0",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "babel-jest": "^26.5.2",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^5.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.5.3",
    "prettier": "^2.1.2",
    "typescript": "^4.2.3"
  }
}
